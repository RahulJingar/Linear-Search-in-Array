#include<iostream>
using namespace std;
int linearSearch(int arr[],int size)
{
    for(int i=0;i<size;i++)
    {
        if(arr[i]==10)
        {
            return i;
        }
    }
    
    return -1;
}
int main()
{
    int arr[6]={2,4,6,8,10,12};
    int size=sizeof(arr)/sizeof(arr[0]);
    cout<<"Found at index "<<linearSearch(arr,size);
}
