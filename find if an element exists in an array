#include<iostream>
using namespace std;
int findtheElement(int arr[],int size)
{
    for(int i=0;i<size;i++)
    {
        if(arr[i]==5)
        {
            cout<<"Found";
        }
    }
    return -1;
}
int main()
{
    int arr[]={1,2,3,4,5,6,7,8,9};
    int size=sizeof(arr)/sizeof(arr[0]);
    findtheElement(arr,size);
}
