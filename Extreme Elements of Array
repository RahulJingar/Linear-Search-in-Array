#include<iostream>
using namespace std;
int extremePrint(int arr[],int size)
{
	int left=0;
	int right=size-1;
	while(left<=right)
	{
		cout<<arr[left]<<" ";
		cout<<arr[right]<<" ";
		left++;
		right--;
	}
	return -1;
}
int main()
{
	int arr[]= {10,20,30,40,50,60};
	int size=sizeof(arr)/sizeof(arr[0]);
	extremePrint(arr,size);
	return 0;
}
